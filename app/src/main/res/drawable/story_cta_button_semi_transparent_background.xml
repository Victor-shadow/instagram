<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/cds_white_a15" />
            <corners
                android:radius="@dimen/abc_dialog_padding_material" />
        </shape>
    </item>
    <item
        android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/primary_text_disabled_material_dark" tools:ignore="PrivateResource" />
            <corners
                android:radius="@dimen/abc_dialog_padding_material" />
        </shape>
    </item>
</selector>