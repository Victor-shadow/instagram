<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_checked="false">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/igds_color_primary_background" />
            <corners
                android:radius="100.0dip" />
        </shape>
    </item>
    <item
        android:state_checked="true">
        <shape
            android:shape="rectangle">
            <solid
                android:color="#ff0e8630" />
            <corners
                android:radius="100.0dip" />
        </shape>
    </item>
</selector>