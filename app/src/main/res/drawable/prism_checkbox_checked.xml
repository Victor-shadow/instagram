<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <size
                android:height="24.0dip"
                android:width="24.0dip" />
            <corners
                android:radius="3.0dip" />
            <solid
                android:color="@color/checkbox_background_color_selector" />
        </shape>
    </item>
    <item
        android:gravity="center"
        android:left="4.0dip"
        android:top="4.0dip"
        android:right="4.0dip"
        android:bottom="4.0dip">
        <bitmap
            android:src="@drawable/instagram_check_pano_filled_16"
            android:tint="@color/checkbox_image_tint" />
    </item>
</layer-list>