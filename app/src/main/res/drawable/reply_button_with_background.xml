<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:gravity="center"
        android:height="24.0dip"
        android:width="24.0dip">
        <shape
            android:shape="oval">
            <solid
                android:color="?igds_color_primary_background" />
        </shape>
    </item>
    <item
        android:gravity="center"
        android:height="20.0dip"
        android:width="20.0dip">
        <shape
            android:shape="oval">
            <solid
                android:color="?igds_color_elevated_highlight_background" />
        </shape>
    </item>
    <item
        android:gravity="center"
        android:height="12.0dip"
        android:width="12.0dip">
        <bitmap
            android:gravity="fill"
            android:src="@drawable/instagram_reply_pano_outline_14"
            android:tint="@color/igds_primary_icon" />
    </item>
</layer-list>