<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <stroke
                android:width="1.0dip"
                android:color="@color/igds_color_primary_text"
                android:dashWidth="5.0dip"
                android:dashGap="5.0dip" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="line">
            <stroke
                android:width="2.0dip"
                android:color="@color/igds_color_primary_text" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="90.0"
            android:toDegrees="-90.0"
            android:pivotX="50.0%"
            android:pivotY="50.0%">
            <shape
                android:shape="line">
                <stroke
                    android:width="2.0dip"
                    android:color="@color/igds_color_primary_text" />
            </shape>
        </rotate>
    </item>
</layer-list>