<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" android:variablePadding="false"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="@color/fbui_border_dark" />
            <stroke
                android:width="1.0dip"
                android:color="@color/fbui_border_dark" />
            <corners
                android:radius="@dimen/abc_action_bar_elevation_material" />
            <padding
                android:left="@dimen/abc_dialog_padding_material"
                android:top="@dimen/action_bar_item_spacing_right"
                android:right="@dimen/abc_dialog_padding_material"
                android:bottom="@dimen/action_bar_item_spacing_right" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/abc_decor_view_status_guard_light" tools:ignore="PrivateResource" />
            <stroke
                android:width="1.0dip"
                android:color="@color/fbui_bg_dark" />
            <corners
                android:radius="@dimen/abc_action_bar_elevation_material" />
            <padding
                android:left="@dimen/abc_dialog_padding_material"
                android:top="@dimen/action_bar_item_spacing_right"
                android:right="@dimen/abc_dialog_padding_material"
                android:bottom="@dimen/action_bar_item_spacing_right" />
        </shape>
    </item>
</selector>