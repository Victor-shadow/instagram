<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/debug_overlay_white_70_transparent" />
            <corners
                android:radius="@dimen/abc_dialog_padding_material" />
        </shape>
    </item>
    <item
        android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/solid_white" />
            <corners
                android:radius="@dimen/abc_dialog_padding_material" />
        </shape>
    </item>
</selector>