<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/transparent" />
            <inset
                android:insetLeft="1.0dip"
                android:insetRight="1.0dip"
                android:insetTop="1.0dip"
                android:insetBottom="1.0dip" />
            <stroke
                android:width="3.0dip"
                android:color="@color/solid_white" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/transparent" />
            <stroke
                android:width="1.0dip"
                android:color="@color/gallery_share_button_bg" />
        </shape>
    </item>
</layer-list>